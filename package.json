{
  "name": "github-time-machine",
  "version": "0.1.1",
  "description": "Time Machine for Github files",
  "repository": {
    "type": "git",
    "url": "git@github.com:patmood/github-time-machine.git"
  },
  "scripts": {
    "start": "node server.js"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascripts/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "~0.7.0",
    "gulp": "3.8.*",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-nodemon": "^1.0.4",
    "gulp-notify": "^1.4.2",
    "gulp-stylus": "~1.3.3",
    "gulp-util": "^3.0.0",
    "handlebars": "^4.5.1",
    "hbsfy": "~2.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "dotenv": "^0.4.0",
    "express": "~3.16.0",
    "jquery": "~2.1.0"
  }
}
